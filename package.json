{
  "name": "@jurijzahn8019/action-dependabot-bump-range",
  "version": "0.0.30",
  "description": "Evaluates the bump range (minor, major, patch) and outputs the metadata",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && for S in rollup; do npm run build:$S; done",
    "build:rollup": "rollup -c",
    "build:types": "tsc -p tsconfig.prod.json -d --outDir ./dist --emitDeclarationOnly",
    "build:tsc": "tsc --build tsconfig.prod.json",
    "build:watch": "tsc -w",
    "build:pkg": "cpy package*.json dist && npm ci --production --prefix dist",
    "clean": "rm -rf ./dist ./tmp",
    "lint": "eslint --fix src/**/*.ts",
    "precommit": "npm run lint && npm run build && git add dist/*",
    "prerel": "standard-version --skip.changelog -p $(git branch --show-current | sed 's/main/rc/g' | sed 's/[_\\/]/-/g')",
    "release": "standard-version",
    "push:tags": "git push --follow-tags origin main",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurijzahn8019/action-dependabot-bump-range.git"
  },
  "author": "Jurij Zahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jurijzahn8019/action-dependabot-bump-range/issues"
  },
  "homepage": "https://github.com/jurijzahn8019/action-dependabot-bump-range#readme",
  "dependencies": {
    "@actions/core": "^1.2.7",
    "@actions/github": "^4.0.0",
    "@octokit/types": "^6.14.2",
    "debug": "^4.3.1",
    "encoding": "^0.1.13",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "8.2.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.41",
    "@types/semver": "^7.3.6",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.23.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "git-branch": "^2.0.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.3.0",
    "rollup": "^2.47.0",
    "standard-version": "^9.2.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
