{
  "name": "@jurijzahn8019/action-dependabot-bump-range",
  "version": "0.0.75",
  "description": "Evaluates the bump range (minor, major, patch) and outputs the metadata",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && for S in rollup; do npm run build:$S; done",
    "build:rollup": "rollup -c",
    "build:types": "tsc -p tsconfig.prod.json -d --outDir ./dist --emitDeclarationOnly",
    "build:tsc": "tsc --noEmit",
    "build:watch": "tsc --noEmit --watch",
    "build:pkg": "cpy package*.json dist && npm ci --production --prefix dist",
    "clean": "rm -rf ./dist ./tmp",
    "lint": "eslint --fix src/**/*.ts",
    "precommit": "npm run lint && npm run build && git add dist/*",
    "prerel": "standard-version --skip.changelog -p $(git branch --show-current | sed 's/main/rc/g' | sed 's/[_\\/]/-/g')",
    "release": "standard-version",
    "push:tags": "git push --follow-tags origin main",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurijzahn8019/action-dependabot-bump-range.git"
  },
  "author": "Jurij Zahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jurijzahn8019/action-dependabot-bump-range/issues"
  },
  "homepage": "https://github.com/jurijzahn8019/action-dependabot-bump-range#readme",
  "dependencies": {
    "@actions/core": "^1.7.0",
    "@actions/github": "^5.0.1",
    "@octokit/types": "^6.34.0",
    "debug": "^4.3.4",
    "encoding": "^0.1.13",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "8.3.2",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "git-branch": "^2.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3"
  }
}
