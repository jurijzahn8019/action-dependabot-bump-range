{
  "name": "@jurijzahn8019/action-dependabot-bump-range",
  "version": "0.0.42",
  "description": "Evaluates the bump range (minor, major, patch) and outputs the metadata",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && for S in rollup; do npm run build:$S; done",
    "build:rollup": "rollup -c",
    "build:types": "tsc -p tsconfig.prod.json -d --outDir ./dist --emitDeclarationOnly",
    "build:tsc": "tsc --noEmit",
    "build:watch": "tsc --noEmit --watch",
    "build:pkg": "cpy package*.json dist && npm ci --production --prefix dist",
    "clean": "rm -rf ./dist ./tmp",
    "lint": "eslint --fix src/**/*.ts",
    "precommit": "npm run lint && npm run build && git add dist/*",
    "prerel": "standard-version --skip.changelog -p $(git branch --show-current | sed 's/main/rc/g' | sed 's/[_\\/]/-/g')",
    "release": "standard-version",
    "push:tags": "git push --follow-tags origin main",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurijzahn8019/action-dependabot-bump-range.git"
  },
  "author": "Jurij Zahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jurijzahn8019/action-dependabot-bump-range/issues"
  },
  "homepage": "https://github.com/jurijzahn8019/action-dependabot-bump-range#readme",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/types": "^6.25.0",
    "debug": "^4.3.2",
    "encoding": "^0.1.13",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "8.2.5",
    "@types/debug": "^4.1.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.7.12",
    "@types/semver": "^7.3.8",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "git-branch": "^2.0.1",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "prettier": "^2.3.2",
    "rollup": "^2.56.3",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
